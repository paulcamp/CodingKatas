@model IEnumerable<RemindersDemo.Models.ReminderItem>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AssignedUser)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssignedUserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EscalationUser)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EscalationUserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OpenStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateDue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OverDue)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AssignedUser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssignedUserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EscalationUser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EscalationUserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OpenStatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateDue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OverDue)
        </td>
        
    </tr>
}

</table>
